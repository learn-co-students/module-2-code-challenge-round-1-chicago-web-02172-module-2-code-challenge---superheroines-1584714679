<h1> Create a Heroine! </h1>
<% if @errors %>
  <% @errors.each do |e| %>
  <p><%= e %></p>
  <% end %>    
<% end %>

<%= form_for @heroine do |f| %>
    Name: <%= f.text_field :name %>
    Super Name: <%= f.text_field :super_name%>
    <%= f.collection_select :powers, Power.order(:name),:id,:name %>
    <%= f.submit "Create" %>
  <% end %>


