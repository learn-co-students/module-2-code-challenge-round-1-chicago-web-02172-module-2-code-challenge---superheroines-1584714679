<h1>New Power Submission Form:</h1>

<% @power.errors.full_messages.each do |error| %>
    <p><%= error %></p>
<% end %>

<%= form_for(@power) do |f| %>
    <label>Power Name: </label><%= f.text_field :name %><br>
    <label>Description: </label><%= f.text_area :description %><br>
    <label>Heroine: </label><%= f.collection_select :heroine_id, @heroines, :id, :super_name %><br>

    <%= f.submit %>
<% end %>
